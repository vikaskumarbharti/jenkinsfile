pipeline {
    agent any

    stages {
        stage('HelloWorld') {
            steps {
                echo 'Hello World'
            }
        }
      stage('Build') {
            steps {
                echo 'Building'
            }
        }
      stage('Test') {
            steps {
                echo 'Testing'
            }
        }
      stage('Deploy') {
            steps {
                echo 'Deploying'
            }
        }
      stage('Release') {
            steps {
                echo 'Releasing'
            }
        }
      stage('Monitor') {
            steps {
                echo 'Monitoring'
            }
        }
    }
}

